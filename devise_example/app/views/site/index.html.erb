<h1>Devise Example App</h1>

<h2>Home Page</h2>

<% if user_signed_in? %>

  <p> Hello, <%= current_user.first_name %>! </p>
  <%= link_to("Log out", destroy_user_session_path, method: "delete") %>

<% else %>

  <%= link_to("Log in", new_user_session_path) %> <br>
  <%= link_to("Sign up", new_user_registration_path) %>

<% end %>
